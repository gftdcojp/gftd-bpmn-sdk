{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core/src/index.ts",
    "packages/dsl/src/index.ts",
    "packages/compiler/src/index.ts",
    "packages/runtime/src/index.ts",
    "packages/importer/src/index.ts",
    "packages/human/src/index.ts",
    "packages/validation/src/index.ts",
    "packages/testing/src/index.ts",
    "packages/ops/src/index.ts"
  ],
  "out": "docs/api",
  "name": "@gftd/bpmn-sdk API Documentation",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": false,
  "excludeNotDocumented": false,
  "hideGenerator": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core Types",
    "DSL",
    "Compiler",
    "Runtime",
    "Importer",
    "Human Tasks",
    "Validation",
    "Testing",
    "Operations",
    "*"
  ],
  "groupOrder": [
    "Modules",
    "Classes",
    "Interfaces",
    "Enums",
    "Type Aliases",
    "Variables",
    "Functions",
    "*"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "cleanOutputDir": true,
  "githubPages": false,
  "theme": "default",
  "readme": "none",
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/gftdcojp/gftd-bpmn-sdk/blob/main/{path}#L{line}",
  "gitRevision": "main",
  "validation": {
    "notDocumented": false,
    "invalidLink": false,
    "notExported": false
  },
  "skipErrorChecking": true,
  "excludeNotDocumented": true
}